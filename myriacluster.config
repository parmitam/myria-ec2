[cluster myriacluster]
KEYNAME = AWSKey
CLUSTER_SIZE = 2
NODE_INSTANCE_TYPE = t1.micro
SPOT_BID = 0.0035
DISABLE_QUEUE=True

PLUGINS = postgresplugin, myriaplugin
CLUSTER_USER = myriaadmin
DNS_PREFIX = True
NODE_IMAGE_ID = ami-765b3e1f
PERMISSIONS = rest

[permission rest]
# this has to be the same as REST_PORT
IP_PROTOCOL = tcp
FROM_PORT = 8753
TO_PORT = 8753

[plugin postgresplugin]
SETUP_CLASS = postgresplugin.PostgresInstaller
PORT = 5401

#new parameters
NEW_EBS_ENABLE=FALSE
NEW_EBS_TYPE=gp2
NEW_EBS_REGION=us-east-1
NEW_EBS_AVAILABILITY_ZONE=us-east-1b
NEW_EBS_SIZE=10

#mount existing
MOUNT_EXISTING_EBS_ENABLE=FALSE
MOUNT_EXISTING_EBS_LIST=vol-2c2f6ac6,vol-c52e6b2f
MOUNT_EXISTING_EBS_REGION=us-east-1

[plugin myriaplugin]
SETUP_CLASS = myriaplugin.MyriaInstaller
POSTGRES_PORT = 5401
